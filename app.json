{
  "expo": {
    "name": "Al-Mahdi Centre",
    "slug": "al-mahdi-centre-london",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splashTest.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.almahdicentre.app",
      "infoPlist": {
        "NSCameraUsageDescription": "No Camera Functionality used within the app."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.almahdicentre.app",
      "permissions": [
        "android.permission.SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      ["expo-notifications",
        {
          "sounds": [
            "./assets/adhaan.wav",
            "./assets/adhaan1.mp3"
          ], 
          "enableBackgroundRemoteNotifications": true
        }
      ]
      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "sounds": [
        "./assets/adhaan.wav"
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "464685b2-2b5d-4171-adb7-37941b3d9c66"
      }
    },
    "owner": "w3plus"
  }
}
